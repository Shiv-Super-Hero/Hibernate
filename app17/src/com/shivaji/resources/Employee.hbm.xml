<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shivaji.pojo.Employee" table="emp1">
		<id name="eno" column="ENO" length="3" type="int"/>
		<property name="ename" column="ENAME" length="10" type="string"/>
		<property name="esal" column="ESAL" length="5" type="float"/>
		<property name="eaddr" column="EADDR" length="10" type="string"/>
		<property name="eemail" column="EEMAIL" length="20" type="string"/>
	</class>
</hibernate-mapping>