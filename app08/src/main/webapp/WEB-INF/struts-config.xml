<!DOCTYPE struts-config PUBLIC
       "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
       "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans>
		<form-bean name="helloForm" type="com.shivaji.formbeans.HelloActionForm"></form-bean>
	</form-beans>
	<action-mappings>
			<action path="/hello" name="helloForm" type="com.shivaji.action.HelloAction">
			<forward name="wish" path="/wish.jsp"/>
		</action>
	</action-mappings>
</struts-config>