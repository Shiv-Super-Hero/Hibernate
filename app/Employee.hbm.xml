<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Employee" table="emp1">
		<id name="eno" column="ENO" length ="3"/>
		<property name="ename" column="ENAME" length="10"/>
		<property name="esal" column="ESAL" length="5"/>
		<property name="eaddr" column="EADDR" length="10"/>
		<property name="eemail" column="EEMAIL" length="20"/>
	</class>
</hibernate-mapping>